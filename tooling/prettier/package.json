    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14"
  },
  "devDependencies": {
    "@vorsteh-queue/tsconfig": "workspace:*",
    "typescript": "5.8.3"
  },
  "prettier": "@vorsteh-queue/prettier-config"
}
