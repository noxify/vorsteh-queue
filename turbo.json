{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["out/**", ".cache/tsbuildinfo.json"]
    },
    "test": {},
    "dev": {
      "persistent": true,
      "cache": false
    },
    "dev:docs": {
      "persistent": true,
      "cache": false
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "outputs": [".cache/.eslintcache"]
    },
    "typecheck": {
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "clean:cache": {
      "cache": false
    }
  },
  "globalEnv": [],
  "globalPassThroughEnv": ["NODE_ENV", "CI", "npm_lifecycle_event"]
}
